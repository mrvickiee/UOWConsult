platform :ios, '9.0'

target 'UOWConsult' do
use_frameworks!
	source 'https://github.com/CocoaPods/Specs.git'

	# https://github.com/pkluz/PKHUD
	pod "PKHUD"

	# https://github.com/jonathantribouharet/JTCalendar
	pod 'JTCalendar', '~> 2.0'
	
	# https://github.com/erichoracek/MSCollectionViewCalendarLayout
	pod 'MSCollectionViewCalendarLayout'

	# https://github.com/skywinder/ActionSheetPicker-3.0
	pod 'ActionSheetPicker-3.0', '~> 2.0.5'

	# https://github.com/MPGNotification/MPGNotification
	pod 'MPGNotification', '~> 1.2'
	
	# https://www.firebase.com/docs/ios/quickstart.html
	pod 'Firebase/Core'
	pod 'Firebase/Database'
	pod 'Firebase/Auth'

end